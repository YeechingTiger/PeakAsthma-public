<div class="row">
  <div class="col-md-4 col-md-offset-8">
    <div class="panel panel-default">
      <div class="panel-body text-center">
        <%= link_to t('tip.views.buttons.create'), new_tip_path, class: 'btn btn-success' %>
      </div>
    </div>
  </div>
</div>
<div class="panel panel-default">
  <div class="panel-body">
    <table class="table" id="symptoms-index-table">
      <thead>
        <tr>
          <th>
            Tips
          </th>
          <th>
            Schedule
          </th>
          <th>
            Action
          </th>
        </tr>
      </thead>
      <tbody>
        <%= render @tips %>
      </tbody>
    </table>
    <%= will_paginate @tips %>
  </div>
</div>
