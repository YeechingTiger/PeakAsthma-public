{
  "generator-rollbread-angular": {
    "viewDirectory": "views/",
    "modalDirectory": "modals/",
    "directiveDirectory": "directives/",
    "filterDirectory": "filters/",
    "serviceDirectory": "services/",
    "factoryDirectory": "factories/",
    "inject": {
      "js": {
        "file": "index.html",
        "marker": "<!-- Add New Component JS Above -->",
        "template": "<script src=\"<%= filename %>\"></script>"
      },
      "scss": {
        "relativeToModule": true,
        "file": "<%= module %>.scss",
        "marker": "/* Add Component SASS Above */",
        "template": "@import \"<%= filename %>\";"
      }
    },
    "modules": [
      {
        "name": "authentication",
        "file": "src/authentication/authentication.js"
      },
      {
        "name": "willowApi",
        "file": "src/willowApi/willowApi.js"
      },
      {
        "name": "mobileComponents",
        "file": "src/mobileComponents/mobileComponents.js"
      },
      {
        "name": "walkthrough",
        "file": "src/walkthrough/walkthrough.js"
      },
      {
        "name": "dashboard",
        "file": "src/dashboard/dashboard.js"
      },
      {
        "name": "asthmaZone",
        "file": "src/asthmaZone/asthmaZone.js"
      },
      {
        "name": "pa.firebase",
        "file": "src/pa.firebase/pa.firebase.js"
      }
    ]
  }
}